{
    "host": "localhost",
    "port": 6379,
    "patterns": [
        {
            "name": "First key",
            "description": "Return user data",
            "key": "user:{0}",
            "type": "HGETALL",
            "redisDb": 16,
            "variables": [
                "UserID"
            ]
        },
        {
            "name": "Second key",
            "description": "Return siteGUID by site name",
            "key": "site:{0}",
            "type": "GET",
            "redisDb": 16,
            "variables": [
                "SiteName"
            ]
        },
        {
            "name": "Third key",
            "description": "Return site's users",
            "key": "site:{0}:users",
            "type": "LIST",
            "redisDb": 16,
            "variables": [
                "SiteGUID"
            ]
        }
    ]
}
